{
    "name": "imedge/rrdgraph",
    "description": "RRDtool Graphing definition parser and renderer",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Gelf",
            "email": "thomas@gelf.net"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "IMEdge\\RrdGraph\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IMEdge\\Tests\\RrdGraph\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.4",
        "ext-ctype": "*"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.4",
        "phpunit/phpunit": "^10 || ^9.5",
        "squizlabs/php_codesniffer": "^3.6",
        "symplify/easy-coding-standard": "^12"
    }
}
